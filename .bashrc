
[ -f "/c/ghcup/env" ] && . "/c/ghcup/env" # ghcup-env

alias v="nvim"
alias vim="nvim"
alias c="clear"
alias ..="cd .."
alias ...="cd ../.."

alias mv="mv -i"
alias cp="cp -i"
alias rm="rm -I --preserve-root"
alias ls="ls --color=auto"
alias grep="grep --color=auto"
alias ll="ls -l"
alias lr="ls -r"
alias rm="rm -rf"
alias ports="netstat -tulanp"

alias note="nvim ~/Downloads/notes.md"
alias q="exit"
alias bashconfig="nvim ~/.bashrc"
alias configgit="nvim ~/.gitconfig"
